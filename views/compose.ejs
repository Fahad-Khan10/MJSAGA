<%-include("partials/header");-%> <%-include("partials/navbar");-%>

<h1 class="home">Compose</h1>

<form action="/home" method="post" class="compose" autocomplete="off" >
  <h2>Title</h2>

  <input type="text" name="compose_title" placeholder="  Enter Title"/>
  <br />
  <br />

  <h4>Post</h4>

  <textarea
    name="compose_text"
    type="text"
    class="form-control"
    placeholder="Write Here..."
    aria-label="Recipient's username"
    cols="5"
    rows="3"
  ></textarea>


  <br />

  <button type="submit" class="btn publish_btn" id="button-addon2">Publish</button>
  
</form>

<%-include("partials/footer");-%>
